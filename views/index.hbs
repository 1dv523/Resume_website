<div class="container-fluid">
  <nav class="navbar navbar-expand-lg navbar-light  sections" id="naviUp">
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
    <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleOb"href="#">Summary</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleSk"  href="#">Skills</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleEx" href="#">Experience</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleEd"   href="#">Education</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#togglePr" href="#">Projects</a></li>
    </ul>
  </div>
</nav>
<div class="row">
    <div class="col-sm-4" id="header">
        <header  >
            <div class="ColContainers" >
                <p style="text-align: center"><img src="{{image}}" alt="Personal Image"
                        style="text-align: center"></p>
                <nav class="contact">
                    <ul>
                        <li><a href="mailto:{{links.mail}}"><i class="fa fa-envelope"></i></a></li>
                        <li><a href={{links.linkdin}}><i class="fa fa-globe"></i></a>
                        </li>
                        <li><a href={{links.github}} title="Github" target="_blank"><i class="fa fa-github"></i></a>
                        </li>
                    </ul>
                </nav>
                
                <div class="card bg-transparent">
                    <h6 class="card-subtitle mb-3">
                       <i class="fa fa-birthday-cake"></i>
                        {{dateOfBirth}}
                    </h6>
                    <h6 class="card-subtitle mb-3">
                        <i class="fa fa-envelope-o"></i>
                        {{contacts.email}}
                    </h6>
                    <h6 class="card-subtitle mb-3">
                        <i class="fa fa-phone"></i>
                        {{contacts.phoneNR}}
                    </h6>
                </div>

                <section class="sections">
                    <div class="containers" >
                        <h4 class="h4"><i class="fa fa-globe"></i> Languages</h4>
                        <section>
                            <ul>
                                {{#if languages}}
                                {{#each languages}}
                                <li>{{this.name }} : {{this.level}} </li>
                                {{/each}}
                                {{/if}}
                            </ul>
                        </section>


                    </div>
                </section> 

                <!-- <a href="/"><i class="fa fa-chevron-up" style="position: fixed"></i></a> -->
               

            </div>
        </header>

    </div>
    <div class="col-sm-8 offset-sm-4">
            

 <nav class="navbar navbar-expand-lg navbar-light  sections" id="naviDown">
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
    <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
  <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleOb"href="#">Summary</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleSk"  href="#">Skills</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleEx" href="#">Experience</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#toggleEd"   href="#">Education</a></li>
     <li class="nav-item"><a class="nav-link" data-toggle="collapse" data-target="#togglePr" href="#">Projects</a></li>
    </ul>
  </div>
</nav>
        <div class="jambotron" style="margin-top: 1%">
            <h1 class="display-4 name">&lt; {{name}} /&gt;</h1>
        </div>

        <h2 style="text-align: center">{{title}}</h2>


        <!-- Summary -->
        <section id="summary" class="section">
            <div class="containers">
                <h4 class="h4" data-toggle="collapse" data-target="#toggleOb"> <i class="fa fa-user-o"></i> Summary</h4>
                <p id="toggleOb">
                    {{objective}}
                </p>
            </div>
        </section>



        <!-- Skills -->
        <section id="skills" class="section Super light green overlay">
            <div class="containers">
                <h4 class="h4" data-toggle="collapse" data-target="#toggleSk"> <i class="fa fa-tasks"></i> Skills</h4>
                <section id="toggleSk">
                    {{#if skills}}
                    {{#each skills}}
                    <i> {{this.name }} &nbsp;&nbsp;</i>
                    {{/each}}
                    {{/if}}
                </section>

            </div>
        </section>

        <!-- Experience -->
        <section id="experience" class="section">
            <div class="containers">
                
                <h4 class="h4" data-toggle="collapse" data-target="#toggleEx"> <i class="fa fa-suitcase"></i> Experience</h4>
                {{#if experince}}
                {{#each experince}}
                <section class="job " id="toggleEx">
                    <h4>{{this.employer}} <span class="date">{{this.startDate}} - {{this.endDate}}</span></h4>
                    <span class="title">{{this.position}}</span>
                    <p><small> {{this.workDescription}} </small>

                    </p>
                </section>
                {{/each}}
                {{/if}}
            </div>
        </section>
        <!-- Education -->
        <section id="education" class="section">
            <div class="containers">
                <h4 class="h4" data-toggle="collapse" data-target="#toggleEd">  <i class="fa fa-graduation-cap"></i>  Education</h4>

                {{#if education}}
                {{#each education}}
                <section class="qualification" id="toggleEd">
                    <h4>{{this.subject}} <span class="date">{{this.startDate}} - {{this.endDate}}</span></h4>
                    <p>{{this.university}}</p>
                </section>
                {{/each}}
                {{/if}}
            </div>
        </section>


        <!-- Additional -->
         <section id="projects" class="section ">
            <div class="containers">
                 <h4 class="h4" data-toggle="collapse" data-target="#togglePr">  <i class="fa fa-wrench"></i>  Projects</h4>

                {{#if projects}}
                {{#each projects}}
                <section class="projects" id="togglePr">
                    <h4 >{{this.title}} <span class="date">{{this.data}}</span></h4>
                    <p>{{this.description}}</p>
                     <p style="font-weight: bold">Technologies used in the project :</p>
                    {{#each this.technologies}}
                    <i> {{this}} &nbsp;&nbsp;</i>
                    {{/each}}
                    <br>
                    <p><a href={{this.link}}>Link to the project</a></p>
                </section>
                {{/each}}
                {{/if}}
            </div>
        </section>

        <section class="">
            <div class="containers">
                <p class="quote">&ldquo;Generally, the craft of programming is the factoring of a set of requirements
                    into a a
                    set of functions and data structures.&rdquo;</p>
                <p class="quote">&ldquo;- Douglas Crockford -&rdquo;</p>
            </div>
        </section>


    </div>

</div>
</div>