<!-- Heading -->



<div class="row ">
    <div class="col-3" id="header">

        <header>
            <div class="containers">
                <p style="text-align: center"><img src="{{image}}" alt="Zacky Kharboutli" style="width: 140px"
                        style="text-align: center"></p>
                <nav class="contact">
                    <ul>
                        <li><a href={{links.mail}}></a><i class="fa fa-envelope"></i></li>
                        <li><a href={{links.linkdin}}><i class="fa fa-globe"></i></a>
                        </li>
                        <li><a href={{links.github}} title="Github" target="_blank"><i class="fa fa-github"></i></a>
                        </li>
                    </ul>
                </nav>



                <nav class="sections">
                    <ul>
                        <li><a href="#summary">Summary</a></li>
                        <li><a href="#skills">Skills</a></li>
                        <li><a href="#experience">Experience</a></li>
                        <li><a href="#education">Education</a></li>
                        <li><a href="#projects">Projects</a></li>
                        <li><a href="#side-projects">Side Projects</a></li>
                    </ul>
                </nav>
            </div>
        </header>

    </div>
    <div class="col-8" style="padding-top:5%">
        <div class="jambotron">
              <h1 class="display-4 name" >&lt;{{name}} /&gt;</h1>
        </div>
      

        <!-- Summary -->
        <section id="summary" class="section ">
            <div class="containers">
                <h3>Summary</h3>
                <p>{{title}}</p>
                <p>
                    {{objective}}
                </p>
            </div>
        </section>

        <!-- Skills -->
        <section id="skills" class="section ">
            <div class="containers">
                <h3>Skills</h3>

                <section>

                    <ul>
                        {{#if skills}}
                        {{#each skills}}
                        <li>{{this.name }} : {{this.level}} </li>
                        {{/each}}
                        {{/if}}
                    </ul>
                </section>

            </div>
        </section>
        <section class="section ">
            <div class="containers">
                <h3>Languages</h3>
                <section>

                    <ul>
                        {{#if languages}}
                        {{#each languages}}
                        <li>{{this.name }} : {{this.level}} </li>
                        {{/each}}
                        {{/if}}
                    </ul>
                </section>


            </div>
        </section>
        <!-- Experience -->
        <section id="experience" class="section">
            <div class="containers">
                <h3>Experience</h3>
                {{#if experince}}
                {{#each experince}}
                <section class="job">
                    <h4>{{this.employer}} <span class="date">{{this.startDate}} - {{this.endDate}}</span></h4>
                    <span class="title">{{this.position}}</span>
                    <p><small> {{this.workDescription}} </small>

                    </p>
                </section>
                {{/each}}
                {{/if}}
            </div>
        </section>
        <!-- Education -->
        <section id="education" class="section">
            <div class="containers">
                <h3>Education</h3>

                {{#if education}}
                {{#each education}}
                <section class="qualification">
                    <h4>{{this.subject}} <span class="date">{{this.startDate}} - {{this.endDate}}</span></h4>
                    <p>{{this.university}}</p>
                </section>
                {{/each}}
                {{/if}}
            </div>
        </section>




        <!-- Additional -->
        <!-- <section id="extra" class="section additional">
            <div class="containers">
                <h2>Extra</h2>
                <ul>
                    <li>Full UK driver's license.</li>
                </ul>
            </div>
        </section> -->

        <section class="section">
            <div class="containers">
                <p class="quote">&ldquo;Generally, the craft of programming is the factoring of a set of requirements
                    into a a
                    set of functions and data structures.&rdquo;</p>
                <p class="quote">&ldquo;- Douglas Crockford -&rdquo;</p>
            </div>
        </section>


    </div>

</div>